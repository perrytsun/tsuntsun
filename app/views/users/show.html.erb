<!--   Fourth block Start   -->
<section id="fourth-block">
  <div class="title-container">
    <h1>mypage</h1>
      <div class="bottom-item">
        <h1><i class="far fa-user-circle"></i></h1>
      </div>
 </section>
  <!--   Fourth block End   -->

 <!--   Fifth block Start   -->
  <section id="fifth-block">
    <h5><p><%= @user.name %></p></h5>
    <p>profile : <%= @user.profile %></p>

      <% if current_user.id == @user.id %>
        <%= link_to "編集する", edit_user_registration_path %>
      <% end %>
   </section>
   <!--   Fifth block End   -->

   <!--   Sixth block Start   -->
   <section id="sixth-block">
  <div class="container">
  <div class="row">
    <div class="col-sm">
        <h2>ユーザーの投稿一覧</h2>
        <% @user.consultations.each do |t| %>
        <%= t.user.name %>
        <%= t.body %>
        <% end %>
    </div>
    <div class="col-sm">
      <h2>ユーザーがいいねした投稿一覧</h2>
      <% @user.liked_consultations.each do |t| %>
      <%= t.user.name %>
    <%= t.body %>
  <% end %>
      </div>
    </div>
  </div>
  </section>
  <!--   Sixth block End   -->
  


<!--   Seventh block Start   -->
<section id="seventh-block">
<p class="info-title">
<%= link_to "topに戻る", consultations_path %>
</p>
</section>
